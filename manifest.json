{
  "name": "YouTUbe Recommendation Limiter",
  "version": "0.0.1",
  "description": "Limit recommendations shown in youtube",
  "permissions": ["storage"],
  "content_scripts": [
    {
      "matches": ["https://*.youtube.com/*"],
      "js": ["contentScript.js"],
      "run_at": "document_end"
    }
  ],
  "manifest_version": 3,
  "action": {
    "default_title": "YouTube Recommendations limiter",
    "default_popup": "popup.html"
  }
}
